{
"env": {
	"browser": true,
	"es2021": true
},
"extends": "eslint:recommended",
"overrides": [
],
"parserOptions": {
	"ecmaVersion": "latest",
	"sourceType": "module"
},
"rules": {
	"indent": [
		"error",
		"tab",
		{ "SwitchCase": 1 }
	],
	"linebreak-style": [
		"error",
		"unix"
	],
	"quotes": [
		"warn",
		"single"
	],
	"semi": [
		"error",
		"never"
	]
}
}
